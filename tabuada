<html>
    <body>
        <?php 
            $x = $_POST["numero"];
            $i=0;
            $r=0;
            for ($i=0;$i<=10;$i++) 
            {
                $r = $x * $i;
                echo "$x X $i = $r<br>" ;
            }
        ?>
    </body>
